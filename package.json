{
  "name": "@tukuyomil032/passgen",
  "version": "1.0.2",
  "description": "A customizable password generator CLI tool",
  "type": "module",
  "main": "./dist/main.js",
  "module": "./dist/main.js",
  "bin": {
    "passgen": "./dist/main.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json && chmod +x dist/main.js",
    "prepare": "pnpm run build",
    "prepublishOnly": "pnpm run build",
    "dev": "ts-node --esm src/main.ts",
    "start": "node dist/main.js",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,md}\""
  },
  "keywords": [
    "password",
    "generator",
    "cli",
    "security",
    "crypto"
  ],
  "author": "tukuyomil032",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tukuyomil032/CLI-passgen.git"
  },
  "bugs": {
    "url": "https://github.com/tukuyomil032/CLI-passgen/issues"
  },
  "homepage": "https://github.com/tukuyomil032/CLI-passgen#readme",
  "dependencies": {
    "clipboardy": "^4.0.0",
    "inquirer": "^9.2.11",
    "ora": "^8.0.1",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.3",
    "@types/node": "^20.10.6",
    "@types/yargs": "^17.0.24",
    "typescript": "^5.9.3",
    "ts-node": "^10.9.1",
    "prettier": "^3.7.4"
  },
  "packageManager": "pnpm@10.27.0"
}
